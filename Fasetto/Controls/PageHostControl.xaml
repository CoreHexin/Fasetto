<UserControl
    x:Class="Fasetto.Controls.PageHostControl"
    xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
    xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
    xmlns:attached="clr-namespace:Fasetto.AttachedProperties"
    xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
    xmlns:local="clr-namespace:Fasetto.Controls"
    xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
    d:DesignHeight="450"
    d:DesignWidth="800"
    mc:Ignorable="d">
    <Grid>
        <Frame x:Name="OldPage" attached:FrameAttachedProperties.NoFrameHistory="True" />
        <Frame x:Name="NewPage" attached:FrameAttachedProperties.NoFrameHistory="True" />
    </Grid>
</UserControl>
